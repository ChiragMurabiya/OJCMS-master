@model eCMS.DataLogic.Models.CaseMemberProfile
@using eCMS.Shared
@{
    ViewBag.Title = "Profile Viewer";
    Layout = "~/Areas/CaseManagement/Views/Shared/_LayoutCase2.cshtml";
}
@section headsection
{
    @Styles.Render("~/assets/plugins/kendo/css")
    @Scripts.Render("~/assets/plugins/kendo/js")
    <script type="text/javascript">
        $(document).ready(function () {
            $('#divContainer :input').attr('disabled', true);
        });
    </script>
}
<div class="container bulk-import">
    <!-- begin: mainpage -->
    <div class="row margin-top-40">
         <div class="col-md-12" id="divContainer">
            @using (Html.BeginForm(Constants.Actions.Edit, Constants.Controllers.CaseMemberProfile, new { area = Constants.Areas.CaseManagement, CaseID = Model.CaseID, CaseMemberID = Model.CaseMemberID }, FormMethod.Post, new { id = "frmEditor", role = "form" }))
            {
                @Html.Partial("_CreateOrEdit", Model)
            }
        </div>
    </div>
</div>
