@model eCMS.DataLogic.Models.CaseAssessment
@using eCMS.Shared
@{
    ViewBag.Title = "Terminate Case Assessment";
    Layout = "~/Areas/CaseManagement/Views/Shared/_LayoutCase2.cshtml";
}
@section headsection
{
    @Styles.Render("~/assets/plugins/kendo/css")
    @Scripts.Render("~/assets/plugins/kendo/js")
}
<div class="container bulk-import">
    <!-- begin: mainpage -->
    <div class="row margin-top-40">
         <div class="col-md-12">
            @using (Html.BeginForm(Constants.Actions.Termination, Constants.Controllers.CaseAssessment, new { area = Constants.Areas.CaseManagement,Id=0, CaseID=Model.CaseID, CaseMemberID=Model.CaseMemberID }, FormMethod.Post, new { id = "frmEditor", role = "form" }))
            {
                @Html.Partial("_Termination", Model)
            }
        </div>
    </div>
</div>